generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgresql://neondb_owner:npg_u5laJvYcx7wp@ep-tiny-block-aiw3rhhw-pooler.c-4.us-east-1.aws.neon.tech/neondb?sslmode=require&channel_binding=require")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}

model Form {
  id        String   @id @default(cuid())
  title     String
  schema    Json
  createdAt DateTime @default(now())
}

model Submission {
  id        Int      @id @default(autoincrement())
  data      Json
  formId    Int
  form      Form     @relation(fields: [formId], references: [id])
  createdAt DateTime @default(now())
}

